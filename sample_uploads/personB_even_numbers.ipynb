    "def even_numbers(tuples):  \n",
    "    new_list = [] \n",
    "    for i in range(len(tuples)): \n",
    "        if tuples[i] % 2 == 0: \n",
    "            new_list.append(tuples[i]) \n",
    "    return new_list \n"
